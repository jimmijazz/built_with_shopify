<body>
  <% include snippets/nav.ejs %>

        <div id="fh5co-services-section">
            <div class="container">
                <div class="row">
                    <div class="col-md-8 col-md-offset-2 text-center heading-section animate-box">
                        <h2>Blog</h2>
                    </div>
                        <% for (var post in blog) { %>
                          <div class="col-md-12 animate-box">
                          <h3 style="margin-bottom: 0px;"><a href="/blog/<%= blog[post].response %>"><%= blog[post].title %></a></h3>
                          <span><%=blog[post].author%></span><span style="margin-left: 10px;"><%=blog[post].date%></span>
                          <p><%=blog[post].description%></p>
                          </div>
                        <% } %>
                </div>
                <div class="row fh5co-services-single">

                    <div class="col-md-12 animate-box">
                        <div class="fh5co-services">
                            <div class="holder-section">


                            </div>
                        </div>


                    </div>


                </div>
            </div>
        </div>
        <% include snippets/footer.ejs %>
  </body>

